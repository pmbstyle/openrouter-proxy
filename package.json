{
  "name": "llm-proxy",
  "version": "1.0.0",
  "description": "High-performance OpenRouter proxy service with REST API and WebSocket streaming",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "load-test": "artillery run tests/load/load-test.yml"
  },
  "keywords": ["openrouter", "llm", "proxy", "api", "websocket", "streaming"],
  "author": "CompanionWorks",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "axios": "^1.6.2",
    "joi": "^17.11.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/joi": "^17.2.3",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "artillery": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
